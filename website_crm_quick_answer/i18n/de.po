msgid ""
"<p>\n"
"            Dear\n"
"            <b>${object.partner_id and object.partner_id.name or\n"
"                 object.contact_name}</b>,\n"
"          </p>\n"
"        <h3>Thanks for contacting us!</h3>\n"
