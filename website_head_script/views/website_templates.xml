<odoo>

    <template id="web_layout" inherit_id="website.layout">
        <xpath expr="//meta[@name='generator']" position="before">
            <t t-if="website and website.head_script_ids">
                <t t-raw="website.apply_head_scripts(main_object)" />
            </t>
        </xpath>
    </template>

</odoo>
