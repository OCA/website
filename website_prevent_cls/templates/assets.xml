<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="assets_editor" inherit_id="website.assets_editor">
        <xpath expr=".">
            <script
                type="text/javascript"
                src="/website_prevent_cls/static/src/js/widget_media.js"
            />
        </xpath>
    </template>
    <template id="assets_wysiwyg" inherit_id="web_editor.assets_editor">
        <xpath expr="." position="inside">
            <script
                type="text/javascript"
                src="/website_prevent_cls/static/src/js/alt_dialog.js"
            />
        </xpath>
    </template>
</odoo>
